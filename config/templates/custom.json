{
    "version": "2",
    "templates": [
        {
            "type": 1,
            "title": "Gitea",
            "description": "A painless self-hosted Git service.",
            "categories": ["SCM"],
            "platform": "linux",
            "logo": "https://secure.gravatar.com/avatar/5603d5a79452bc237ac44f663d39f9aa.jpg?s=120",
            "image": "gitea/gitea:latest",
            "ports": [
            "3000/tcp", "22/tcp"
            ],
            "volumes": [{"container": "/data"}],
            "restart_policy": "unless-stopped"
        }
    ]
}